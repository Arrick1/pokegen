<% for (let i = 0; i < user.cards.length; i++) {%>

    <div id="card-body">

        <div id="name"><%=cards[i].name%>></div>
        <div id="hp-box"><font size="2px">HP</font> <%=cards[i].hp%></div>

        <div id="img-box-border">
            <div id="img-box">
                <img src="<%=cards[i].url%>">
            </div>
        </div>

        

        <div id="attack-box">
            <div id="attack"><%=cards[i].attack%></div>
            <div id="damage"><%=cards[i].damagae%></div>
        </div>

        <div id="weak-resist-box">
            <div id="weakness">Weakness</div><br/>
            <small><%=cards[i].weakness%></small>
            <div id="divider"></div>
            <div id="resistance">Resistance</div>
            <small><%=cards[i].resistance%></small>
        </div>

    </div>

<% } %>